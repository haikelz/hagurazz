    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "encoding": "^0.1.13",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "^9.0.0",
