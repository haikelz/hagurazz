{
  "name": "haikel",
  "author": "Haikel Ilham Hakim",
  "version": "1.7.0",
  "private": true,
  "description": "My personal Website",
  "homepage": "https://ekel.dev/",
  "packageManager": "pnpm@8.6.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/haikelz/haikel/"
  },
  "scripts": {
    "commit": "cz",
    "prepare": "husky install",
    "dev": "next dev",
    "lint": "next lint",
    "format": "prettier . --write",
    "build": "next build",
    "db:migrate": "tsx drizzle/db/migrate",
    "db:introspect": "drizzle-kit introspect:sqlite",
    "db:generate": "drizzle-kit generate:sqlite --breakpoints --out drizzle/migrations --schema=./drizzle/db/schema.ts",
    "open": "cypress open",
    "test": "cypress run"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@libsql/client": "^0.7.0",
    "@marsidev/react-turnstile": "^0.7.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.1",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "clsx": "^2.1.1",
    "contentlayer": "^0.3.4",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.31.2",
    "fast-fuzzy": "^1.12.0",
    "framer-motion": "^11.2.13",
    "jotai": "^2.8.4",
    "lucide-react": "^0.400.0",
    "next": "^14.2.4",
    "next-auth": "^4.24.7",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.3.0",
    "ofetch": "^1.3.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.52.1",
    "react-string-replace": "^1.1.1",
    "reading-time": "^1.5.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "3.0.1",
    "rss": "^1.2.2",
    "tailwind-merge": "^2.3.0",
    "ts-pattern": "^5.2.0",
    "yet-another-react-lightbox": "^3.21.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/rss": "^0.0.32",
    "@unocss/postcss": "^0.61.3",
    "@unocss/preset-wind": "^0.61.3",
    "@unocss/reset": "^0.61.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.22.8",
    "eslint": "^9.6.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "7.34.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rehype-shikiji": "^0.10.2",
    "tsx": "^4.16.2",
    "turbo": "^2.0.6",
    "typescript": "5.5.3",
    "unocss": "^0.61.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && pnpm cz --hook || true"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./prettier.config.js --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./prettier.config.js --write"
    ]
  }
}
